{
  "metadata": [
    {
      "src": [
        {
          "src": "src",
          "files": ["**/*.csproj"],
          "exclude": ["**/bin/**", "**/obj/**"]
        }
      ],
      "dest": "api",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "flattened",
      "memberLayout": "samePage",
      "allowCompilationErrors": false
    }
  ],
  "build": {
    "content": [
      {
        "files": ["api/**.yml", "api/index.md"]
      },
      {
        "files": ["**/*.md", "**/*.yml"],
        "src": "docs"
      }
    ],
    "resource": [],
    "output": "_apidocs",
    "template": ["default", "modern"],
    "globalMetadata": {
      "_appTitle": "ANDO API Documentation",
      "_appName": "ANDO",
      "_appFooter": "ANDO - Typed C# Build System",
      "_enableSearch": true,
      "_enableNewTab": true
    },
    "fileMetadata": {},
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}
